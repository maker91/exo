function map(func, l) {
    r = []
    for i=0, #l-1 {
        r[i] = func(l[i])
    }
    return r
}

function reduce(func, l, init) {
    for i=0, #l-1
        init = func(init, l[i])
    return init
}

function zip(a, b) {
    m = {}
    for i=0, #a-1
	m[a[i]] = b[i]
    return m
}

function add_one(x) {
    return x+1
}

add = function(a, b) {
    return a+b
}

a = [1, 3, 5, 7, 9]
b = map(add_one, a)
c = zip(a, b)
print(a)
print(b)
print(c)

r = reduce(add, a, 0)
print(r)
